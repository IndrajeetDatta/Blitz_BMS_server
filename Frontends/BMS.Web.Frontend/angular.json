{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bms-web-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/bms-web-frontend",
      "sourceRoot": "projects/bms-web-frontend/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bms-web-frontend",
            "index": "projects/bms-web-frontend/src/index.html",
            "main": "projects/bms-web-frontend/src/main.ts",
            "polyfills": "projects/bms-web-frontend/src/polyfills.ts",
            "tsConfig": "projects/bms-web-frontend/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/bms-web-frontend/src/favicon.ico",
              "projects/bms-web-frontend/src/assets"
            ],
            "styles": [
              "projects/bms-web-frontend/src/styles.scss",
              "node_modules/datatables.net-bs5/css/dataTables.bootstrap5.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js",
              "node_modules/datatables.net-bs5/js/dataTables.bootstrap5.min.js",
              "node_modules/jquery/dist/jquery.min.js", 
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/bms-web-frontend/src/environments/environment.ts",
                  "with": "projects/bms-web-frontend/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "dev": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "500kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/bms-web-frontend/src/environments/environment.ts",
                  "with": "projects/bms-web-frontend/src/environments/environment.dev.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "bms-web-frontend:build:production"
            },
            "development": {
              "browserTarget": "bms-web-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bms-web-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bms-web-frontend/src/test.ts",
            "polyfills": "projects/bms-web-frontend/src/polyfills.ts",
            "tsConfig": "projects/bms-web-frontend/tsconfig.spec.json",
            "karmaConfig": "projects/bms-web-frontend/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/bms-web-frontend/src/favicon.ico",
              "projects/bms-web-frontend/src/assets"
            ],
            "styles": [
              "projects/bms-web-frontend/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "bms-web-api-client": {
      "projectType": "library",
      "root": "projects/bms-web-api-client",
      "sourceRoot": "projects/bms-web-api-client/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/bms-web-api-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/bms-web-api-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/bms-web-api-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bms-web-api-client/src/test.ts",
            "tsConfig": "projects/bms-web-api-client/tsconfig.spec.json",
            "karmaConfig": "projects/bms-web-api-client/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}